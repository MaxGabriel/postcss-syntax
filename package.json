{
  "name": "PostCSS",
  "displayName": "PostCSS syntax",
  "description": "PostCSS language support",
  "version": "1.0.0",
  "publisher": "rcsole",
  "engines": {
    "vscode": "^0.10.10"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "postcss",
        "aliases": [
          "PostCSS",
          "postcss"
        ],
        "extensions": [
          ".pcss",
          ".postcss"
        ],
        "configuration": "./postcss.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "postcss",
        "scopeName": "source.postcss",
        "path": "./syntaxes/postcss.tmLanguage"
      }
    ]
  }
}
